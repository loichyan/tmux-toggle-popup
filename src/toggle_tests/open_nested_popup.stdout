TMUX:BEGIN[1] {
	TMUX=tmux-test
	TMUX_POPUP_SERVER=
	SHELL=/system/shell
}
	run
	-C
	run "#{@popup_before_open}"
	;
	display-popup
	-e
	TMUX_POPUP_SERVER=popup_test
	TMUX:BEGIN[2] {
		TMUX=tmux-test
		TMUX_POPUP_SERVER=
		SHELL=/system/shell
	}
		-L
		popup_test
	TMUX:END[2]
	;
	run
	-C
	run "#{@popup_after_close}"
	;
TMUX:END[1]
