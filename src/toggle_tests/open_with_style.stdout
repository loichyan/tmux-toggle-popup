TMUX:BEGIN[1] {
	TMUX=tmux-test
	TMUX_POPUP_SERVER=
	SHELL=/system/shell
}
	run
	-C
	run '#{@popup_before_open}'
	;
	display-popup
	-x
	R
	-y
	P
	-w
	50%
	-h
	70%
	TMUX:BEGIN[2] {
		TMUX=tmux-test
		TMUX_POPUP_SERVER=popup_test
		SHELL=/default/shell
	}
		-L
		popup_test
		new
		-As
		default_id_format/p_open_with_style
		-e
		__tmux_popup_name=p_open_with_style
		;
		setenv
		__tmux_popup_caller
		%current_pane_id:tmux-test
		;
		run
		-C
		run '#{@popup_on_init}'
		;
	TMUX:END[2]
	;
	run
	-C
	run '#{@popup_after_close}'
	;
TMUX:END[1]
