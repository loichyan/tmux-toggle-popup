TMUX:BEGIN[1] {
	TMUX=tmux-test
	TMUX_POPUP_SERVER=
	SHELL=/system/shell
}
	run
	-C
	run '#{@popup-focus} --enter'
	;
	set
	default-shell
	/bin/sh
	;
	display-popup
	TMUX:BEGIN[2] {
		TMUX=tmux-test
		TMUX_POPUP_SERVER=
		SHELL=/system/shell
	}
		set
		default-shell
		/default/shell
	TMUX:END[2]
	TMUX:BEGIN[3] {
		TMUX=tmux-test
		TMUX_POPUP_SERVER=popup_server
		SHELL=/default/shell
	}
		-S
		/local/socket_path/popup_server
		new
		-As
		local_id_format/p_open_with_overrides
		-e
		__tmux_popup_name=p_open_with_overrides
		;
		setenv
		__tmux_popup_caller
		%current_pane_id:tmux-test
		;
	TMUX:END[3]
	;
	run
	-C
	run '#{@popup-focus} --leave'
	;
TMUX:END[1]
