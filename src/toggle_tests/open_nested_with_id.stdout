TMUX:BEGIN[1] {
	TMUX=tmux-test
	TMUX_POPUP_SERVER=
	SHELL=/system/shell
}
	run
	#{@popup_before_open}
	;
	set
	default-shell
	/bin/sh
	;
	display-popup
	TMUX:BEGIN[2] {
		TMUX=tmux-test
		TMUX_POPUP_SERVER=
		SHELL=/system/shell
	}
		set
		default-shell
		/default/shell
	TMUX:END[2]
	TMUX:BEGIN[3] {
		TMUX=tmux-test
		TMUX_POPUP_SERVER=popup_test
		SHELL=/default/shell
	}
		-L
		popup_test
	TMUX:END[3]
	;
	run
	#{@popup_after_close}
	;
TMUX:END[1]
