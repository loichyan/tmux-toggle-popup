TMUX:BEGIN[1] {
	TMUX=tmux-test
	TMUX_POPUP_SERVER=
	SHELL=/system/shell
}
	run-shell
	-C
	run "#{@popup_before_open}"
	;
	display-popup
	-e
	TMUX_POPUP_SERVER=popup_test
	TMUX:BEGIN[2] {
		TMUX=tmux-test
		TMUX_POPUP_SERVER=
		SHELL=/system/shell
	}
		-L
		popup_test
		new-session
		-As
		p_open_with_id
		-e
		__tmux_popup_name=p_open_with_id
		;
		setenv
		__tmux_popup_caller
		%current_pane_id:tmux-test
		;
		run-shell
		-C
		run "#{@popup_on_init}"
		;
	TMUX:END[2]
	;
	run-shell
	-C
	run "#{@popup_after_close}"
	;
TMUX:END[1]
