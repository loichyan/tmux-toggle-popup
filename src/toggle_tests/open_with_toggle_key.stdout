>>>TMUX:BEGIN[1]
	run
	#{@popup_before_open}
	;
	set
	default-shell
	/bin/sh
	;
	display-popup
	>>>TMUX:BEGIN[2]
		set
		default-shell
		/default/shell
	<<<TMUX:END[2]
	>>>TMUX:BEGIN[3]
		-L
		popup
		new
		-As
		default_id_format/p_toggle_key
		;
		set
		@__popup_name
		p_toggle_key
		;
		set
		@__popup_id_format
		default_id_format/{popup_name}
		;
		set
		@__popup_caller_path
		working_session_path
		;
		set
		@__popup_caller_pane_path
		working_pane_path
		;
		bind
		-T
		root
		M-p
		run
		#{@popup-toggle} --name=p_toggle_key --toggle-key=-T\ root\ M-p --toggle-key=-n\ M-o
		;
		bind
		-n
		M-o
		run
		#{@popup-toggle} --name=p_toggle_key --toggle-key=-T\ root\ M-p --toggle-key=-n\ M-o
		;
		run
		#{@popup_on_init}
	<<<TMUX:END[3]
	;
	run
	#{@popup_after_close}
	;
<<<TMUX:END[1]
>>>TMUX:BEGIN[4]
	-N
	-L
	popup
	unbind
	-T
	root
	M-p
	;
	unbind
	-n
	M-o
	;
<<<TMUX:END[4]
