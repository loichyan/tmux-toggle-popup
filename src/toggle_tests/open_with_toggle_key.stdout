TMUX:BEGIN[1] {
	TMUX=tmux-test
	TMUX_POPUP_SERVER=
	SHELL=/system/shell
}
	run
	-C
	run '#{@popup_before_open}'
	;
	display-popup
	-e
	TMUX_POPUP_SERVER=popup_test
	TMUX:BEGIN[2] {
		TMUX=tmux-test
		TMUX_POPUP_SERVER=
		SHELL=/system/shell
	}
		-L
		popup_test
		new
		-As
		default_id_format/p_toggle_key
		-e
		__tmux_popup_name=p_toggle_key
		;
		setenv
		__tmux_popup_caller
		%current_pane_id:tmux-test
		;
		bind
		-T
		root
		M-p
		run
		#{@popup-toggle} --name=p_toggle_key --toggle-key=-T\ root\ M-p --toggle-key=-n\ M-o
		;
		bind
		-n
		M-o
		run
		#{@popup-toggle} --name=p_toggle_key --toggle-key=-T\ root\ M-p --toggle-key=-n\ M-o
		;
		run
		-C
		run '#{@popup_on_init}'
		;
	TMUX:END[2]
	;
	run
	-C
	run '#{@popup_after_close}'
	;
TMUX:END[1]
TMUX:BEGIN[3] {
	TMUX=tmux-test
	TMUX_POPUP_SERVER=
	SHELL=/system/shell
}
	-N
	-L
	popup_test
	unbind
	-T
	root
	M-p
	;
	unbind
	-n
	M-o
	;
TMUX:END[3]
