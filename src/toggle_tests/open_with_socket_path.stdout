TMUX:BEGIN[1] {
	TMUX=tmux-test
	TMUX_POPUP_SERVER=
	SHELL=/system/shell
}
	run
	#{@popup_before_open}
	;
	set
	default-shell
	/bin/sh
	;
	display-popup
	TMUX:BEGIN[2] {
		TMUX=tmux-test
		TMUX_POPUP_SERVER=
		SHELL=/system/shell
	}
		set
		default-shell
		/default/shell
	TMUX:END[2]
	TMUX:BEGIN[3] {
		TMUX=tmux-test
		TMUX_POPUP_SERVER=socket_path_server
		SHELL=/default/shell
	}
		-S
		/path/to/socket_path_server
		new
		-As
		default_id_format/p_open_with_socket_path
		-e
		__tmux_popup_name=p_open_with_socket_path
		;
		setenv
		__tmux_popup_caller
		/current/socket/path:%current_pane_id
		;
		run
		#{@popup_on_init}
	TMUX:END[3]
	;
	run
	#{@popup_after_close}
	;
TMUX:END[1]
